	keywords = {chlorophyll fluorescence, leaf respiration, low temperature},
	pages = {1513–1522},
	file = {Full Text PDF:/home/kevin/uw/zotero/storage/NNRCN8QK/Armstrong et al. - 2007 - Assessing the relationship between respiratory acc.pdf:application/pdf}
},

