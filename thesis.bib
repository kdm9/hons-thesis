	abstract = {Accessions of a plant species can show considerable genetic differences that are analyzed effectively by using recombinant inbred line ({RIL)} populations. Here we describe the results of genome-wide expression variation analysis in an {RIL} population of Arabidopsis thaliana. For many genes, variation in expression could be explained by expression quantitative trait loci ({eQTLs).} The nature and consequences of this variation are discussed based on additional genetic parameters, such as heritability and transgression and by examining the genomic position of {eQTLs} versus gene position, polymorphism frequency, and gene ontology. Furthermore, we developed an approach for genetic regulatory network construction by combining {eQTL} mapping and regulator candidate gene selection. The power of our method was shown in a case study of genes associated with flowering time, a well studied regulatory network in Arabidopsis. Results that revealed clusters of coregulated genes and their most likely regulators were in agreement with published data, and unknown relationships could be predicted.},
	language = {en},
	number = {5},
	urldate = {2013-01-10},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Keurentjes, Joost J. B. and Fu, Jingyuan and Terpstra, Inez R. and Garcia, Juan M. and Ackerveken, Guido van den and Snoek, L. Basten and Peeters, Anton J. M. and Vreugdenhil, Dick and Koornneef, Maarten and Jansen, Ritsert C.},
	month = jan,
	year = {2007},
	keywords = {natural variation},
	pages = {1708--1713},
	file = {Keurentjes et al. - 2007 - Regulatory network construction in Arabidopsis by .pdf:/home/kevin/uw/zotero/storage/G3RWWSUH/Keurentjes et al. - 2007 - Regulatory network construction in Arabidopsis by .pdf:application/pdf}
