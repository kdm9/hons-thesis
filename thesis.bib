	keywords = {Calibration, Comparative Ct method, {DNA}, Complementary, Fluorescent Dyes, Linear Models, Linear regression, {mRNA} quantification, Organic Chemicals, {PCR} efficiency, Polymerase Chain Reaction, Quantitative polymerase chain reaction ({PCR)}, Real-time {PCR}, {RNA}, Messenger, Standard curve method, {SYBR®} green I, Time Factors},
	pages = {62--66},
	file = {ScienceDirect Full Text PDF:/home/kevin/uw/zotero/storage/AMHGDGCA/Ramakers et al. - 2003 - Assumption-free analysis of quantitative real-time.pdf:application/pdf;ScienceDirect Full Text PDF:/home/kevin/uw/zotero/storage/NJF59GA9/Ramakers et al. - 2003 - Assumption-free analysis of quantitative real-time.pdf:application/pdf;ScienceDirect Snapshot:/home/kevin/uw/zotero/storage/UPQ87HI3/S0304394002014234.html:text/html;ScienceDirect Snapshot:/home/kevin/uw/zotero/storage/FW4RKKES/S0304394002014234.html:text/html}
